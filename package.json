{
  "name": "channel-aggregation-decisioning-processor",
  "version": "0.1.0",
  "description": "channel-aggregation-decisioning-processor",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "cleanup": "rm -rf dist template jest.config.js jest.config.js.map node_modules",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "prepare": "husky install",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Johan Foley"
    },
    {
      "name": "Jean-Pierre Nell"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@frmscoe/frms-coe-lib": "^0.4.3",
    "@frmscoe/frms-coe-startup-lib": "github:cshezi/startup-lib-nats-from",
    "@log4js-node/logstash-http": "^1.1.0",
    "dotenv": "^16.3.1",
    "log4js": "^6.4.0",
    "node-cache": "^5.1.2",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/jest": "^29.5.1",
    "@types/node-cache": "^4.2.5",
    "@types/node-fetch": "^2.5.8",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "husky": "^8.0.0",
    "ioredis-mock": "^8.7.0",
    "jest": "^29.5.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.0",
    "replace": "1.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
